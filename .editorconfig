# Base v1
root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
insert_final_newline = true
charset = utf-8
trim_trailing_whitespace = true

[*.{cs,sln}]
indent_size = 4
charset = utf-8-bom

[*.csproj]
charset = utf-8-bom

[*.go]
indent_style = tab
tab_width = 4

[*.md]
indent_size = 4
trim_trailing_whitespace = false
